client
dev tun
proto udp
remote vpn.luceatmail.com 1194
resolv-retry infinite
nobind
cipher AES-256-CBC
auth SHA512
comp-lzo
verb 3

#tun-mtu 1500
#tun-mtu-extra 32
#mssfix 1450
persist-key
persist-tun

#ping 15
#ping-restart 0
#ping-timer-rem
#reneg-sec 0
mute 20

#remote-cert-tls server
auth-user-pass
auth-nocache

#pull
#fast-io
#key-direction 1

